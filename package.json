{
	"name": "phishyapps.com",
	"description": "Automatically remove malicious phishing apps from the iTunes App Store & Google Play Store",
	"version": "0.0.1",
	"domain": "phishyapps.com",
	"scripts": {
		"postinstall": "npm run fix:types",
		"fix:types": "rimraf node_modules/vuex/types/vue.d.ts",
		"clean": "rimraf dist node_modules",
		"client:dev": "cross-env NODE_ENV=development vue-cli-service serve --mode development",
		"client:dev:legacy": "rimraf dist/client && cross-env NODE_ENV=development webpack --config node_modules/@vue/cli-service/webpack.config.js",
		"client:build": "rimraf dist/client && cross-env NODE_ENV=production vue-cli-service build --mode production",
		"client:deploy": "npm run client:build && sudo rm -rf /var/www/phishyapps.com/client && sudo cp -r dist/client /var/www/phishyapps.com && sudo chown -R www-data:www-data /var/www/phishyapps.com && sudo chmod -R 755 /var/www/phishyapps.com",
		"server:dev": "rimraf dist/server && tsc-watch -p config/.server.tsconfig.json --onSuccess 'cross-env NODE_ENV=development node dist/server/main.js'",
		"server:deploy": "rimraf dist/server && tsc -p config/.server.tsconfig.json && npm run server:start",
		"server:start": "forever stop phishyapps.com || true && rimraf ~/.forever/phishyapps.com.log && cross-env NODE_ENV=production forever start config/forever.json && npm run server:logs",
		"server:logs": "tail -F ~/.forever/phishyapps.com.log"
	},
	"dependencies": {
		"@ibm/plex": "latest",
		"ajv": "latest",
		"animate.css": "latest",
		"app-store-scraper": "latest",
		"boom": "latest",
		"buefy": "latest",
		"bulma": "latest",
		"cookie": "latest",
		"cors": "latest",
		"cron": "latest",
		"eventemitter3": "latest",
		"fastify": "latest",
		"fastify-cookie": "latest",
		"fingerprintjs2": "latest",
		"fuzzy": "latest",
		"google-play-scraper": "latest",
		"got": "latest",
		"ibm-design-colors": "latest",
		"ioredis": "latest",
		"lockr": "latest",
		"lodash": "latest",
		"mdi": "latest",
		"moment": "latest",
		"node-forge": "latest",
		"register-service-worker": "latest",
		"uws": "latest",
		"vue": "latest",
		"vue-class-component": "latest",
		"vue-property-decorator": "latest",
		"vue-router": "latest",
		"vuex": "latest"
	},
	"devDependencies": {
		"@types/boom": "latest",
		"@types/clipboardy": "latest",
		"@types/cookie": "latest",
		"@types/cors": "latest",
		"@types/cron": "latest",
		"@types/dotenv": "latest",
		"@types/eyes": "latest",
		"@types/got": "latest",
		"@types/ioredis": "latest",
		"@types/lockr": "latest",
		"@types/lodash": "latest",
		"@types/node": "latest",
		"@types/node-forge": "latest",
		"@types/source-map-support": "latest",
		"@types/uws": "latest",
		"@vue/cli-plugin-pwa": "latest",
		"@vue/cli-plugin-typescript": "latest",
		"@vue/cli-service": "latest",
		"chalk": "latest",
		"clipboardy": "latest",
		"cross-env": "latest",
		"debug-trace": "latest",
		"dotenv": "latest",
		"dts-gen": "latest",
